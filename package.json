{
  "name": "pastelight",
  "productName": "pastelight",
  "version": "1.0.0-beta",
  "author": "deseteral@gmail.com",
  "license": "MIT",
  "main": "build/main.js",
  "scripts": {
    "start": "electron build/main.js",
    "dev": "npm run build && npm run compile:watch",
    "build": "npm run clean && npm run build:assets && npm run compile",
    "build:assets": "gulp",
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "clean": "rm -rf build",
    "test": "npm run unit && npm run unit:eslint-rules && npm run lint",
    "unit": "node ./config/run-unit-tests.js",
    "unit:eslint-rules": "node ./config/eslint-plugin-pastelight/tests.js",
    "lint": "eslint src --ext js,jsx,ts,tsx"
  },
  "dependencies": {
    "dms2dec": "1.1.0",
    "electron": "7.0.0",
    "electron-devtools-installer": "2.2.4",
    "exif": "0.6.0",
    "image-size": "0.8.3",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-redux": "7.1.1",
    "recursive-readdir": "2.2.2",
    "redux": "4.0.4",
    "styled-components": "4.4.0"
  },
  "devDependencies": {
    "@deseteral/eslint-config": "1.3.1",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/exif": "0.6.0",
    "@types/jest": "24.0.20",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-redux": "7.1.5",
    "@types/recursive-readdir": "2.2.0",
    "@types/styled-components": "4.1.19",
    "coveralls": "3.0.7",
    "eslint": "5.16.0",
    "eslint-plugin-pastelight": "file:config/eslint-plugin-pastelight",
    "gulp": "4.0.2",
    "gulp-replace": "1.0.0",
    "jest": "24.9.0",
    "jest-watch-typeahead": "0.4.0",
    "ts-jest": "24.1.0",
    "typescript": "3.6.4"
  }
}
