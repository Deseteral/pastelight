    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-hot-loader": "4.1.3",
    "react-redux": "5.0.7",
    "recursive-readdir": "2.2.2",
    "redux": "4.0.0",
    "styled-components": "3.2.6"
  },
  "devDependencies": {
    "babel-jest": "23.2.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "electron-forge": "5.2.2",
    "electron-prebuilt-compile": "2.0.2",
