{
  "env": {
    "development": {
      "text/typescript": {
        "removeComments": false,
        "preserveConstEnums": true,
        "declaration": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noUnusedParameters": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "experimentalDecorators": true,
        "target": "es2015",
        "module": "commonjs",
        "jsx": "react"
      }
    },
    "production": {
      "text/typescript": {
        "removeComments": false,
        "preserveConstEnums": true,
        "declaration": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noUnusedParameters": true,
        "sourceMap": false,
        "experimentalDecorators": true,
        "target": "es2015",
        "jsx": "react"
      }
    }
  }
}
